  new-expr:
    name: new.expr.tsx
    begin: '\b(new)\b'
    beginCaptures:
      '1': { name: keyword.others.tsx }
    # new-expr ends just before any type or call arguments. Type arguments will be picked up by #cast.
    end: '(?=[(;]|$)'
    patterns:
    - include: '#type'
    - include: '#comment'

  object-member:
    name: meta.object.member.tsx
    begin: '[a-zA-Z_$][\w$]*\s*:'
