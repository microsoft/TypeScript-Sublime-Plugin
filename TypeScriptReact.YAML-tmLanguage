      '5': { name: keyword.operator.tsx } # captures keyword (new)
      '6': { name: storage.type.tsx } # captures keyword (constructor)
      '7': { name: entity.name.function.tsx } # captures method name ([a-zA-Z_$][\.\w$]*)
      '8': { name: keyword.operator.tsx } # captures (\??)

