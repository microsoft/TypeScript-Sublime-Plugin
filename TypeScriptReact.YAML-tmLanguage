repository:
  var-expr:
    name: meta.var.expr.tsx
    begin: (?<!\()\s*\b(var|let|const(?!\s+enum))\b
    beginCaptures:
      '1': { name: storage.type.tsx }
    end: (?=$|;)
    patterns:
    - include: '#var-single-variable'
    - include: '#comment'

  var-single-variable:
    name: meta.var-single-variable.expr.tsx
    begin: \b([a-zA-Z_$][\w$]*)\s*(=?)
    beginCaptures:
      '1': { name: variable.tsx }
    end: (?=$|[;,])
    patterns:
    - include: '#ternary-expression'
    - include: '#type-annotation'
    - include: '#string'
    - include: '#comment'
    - include: '#expression'

  ternary-expression:
    begin: (?=\?)
    end: (?=$|[;,])
    patterns:
    - include: '#ternary-operator'
    - include: '#expression-type' 

  ternary-operator:
    begin: (\?)
    end: (:)
    patterns:
    - include: '#expression-type' 

